# Applies Min-Max Normalization
min_max_normalize <- function(x) {
(x - min(x)) / (max(x) - min(x))
}
z_score_standardize <- function(x) {
(x - mean(x)) / sd(x)
}
robust_scale <- function(x) {
(x - median(x)) / IQR(x)
}
